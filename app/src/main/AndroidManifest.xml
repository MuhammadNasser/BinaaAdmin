<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.binaa.binaaadmin">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".utils.ApplicationBase"
        android:icon="@drawable/binaa_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/binaa_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccountActivity" />
        <activity android:name=".ReservationDetailsActivity" />
        <activity android:name=".DetailsActivity" />

        <service android:name=".utils.MyFirebaseInstanceIDService" />
        <service android:name=".utils.MyFirebaseMessagingService" />
    </application>

</manifest>